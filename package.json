{
  "name": "liftoff",
  "displayName": "Liftoff - Autonomous AI Agents",
  "description": "Autonomous multi-agent coding system. You give tasks, it plans and executes with specialized agents (frontend, backend, testing, browser). Retry on failure, TODO tracking, real tool execution.",
  "version": "0.1.0",
  "publisher": "jamie",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "liftoff.openManager",
        "title": "Liftoff: Open Panel (Full Tab)"
      },
      {
        "command": "liftoff.openSidebar",
        "title": "Liftoff: Open Sidebar"
      },
      {
        "command": "liftoff.spawnAgent",
        "title": "Liftoff: Spawn New Agent"
      },
      {
        "command": "liftoff.killAllAgents",
        "title": "Liftoff: Stop All Agents"
      },
      {
        "command": "liftoff.continueAgent",
        "title": "Liftoff: Continue Agent Conversation"
      },
      {
        "command": "liftoff.viewHistory",
        "title": "Liftoff: View Session History"
      },
      {
        "command": "liftoff.showOutput",
        "title": "Liftoff: Show Output Panel"
      },
      {
        "command": "liftoff.setApiKey",
        "title": "Liftoff: Set HuggingFace API Key"
      },
      {
        "command": "liftoff.testMcpTools",
        "title": "Liftoff: Test MCP Tools"
      },
      {
        "command": "liftoff.initMcp",
        "title": "Liftoff: Initialize MCP Config"
      },
      {
        "command": "liftoff.orchestratorChat",
        "title": "Liftoff: Open Orchestrator Chat"
      },
      {
        "command": "liftoff.buildApp",
        "title": "Liftoff: Build App from Description"
      },
      {
        "command": "liftoff.resumeBuild",
        "title": "Liftoff: Resume Interrupted Build"
      },
      {
        "command": "liftoff.addFeature",
        "title": "Liftoff: Add Feature to App"
      },
      {
        "command": "liftoff.deployApp",
        "title": "Liftoff: Deploy App"
      },
      {
        "command": "liftoff.generateTests",
        "title": "Liftoff: Generate Tests for This Project"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "liftoff",
          "title": "Liftoff",
          "icon": "resources/rocket.svg"
        }
      ]
    },
    "views": {
      "liftoff": [
        {
          "type": "webview",
          "id": "liftoff.managerView",
          "name": "Agent Manager"
        },
        {
          "type": "webview",
          "id": "liftoff.artifactView",
          "name": "Artifacts"
        }
      ]
    },
    "menus": {
      "commandPalette": [
        {
          "command": "liftoff.openManager"
        },
        {
          "command": "liftoff.openSidebar"
        },
        {
          "command": "liftoff.spawnAgent"
        },
        {
          "command": "liftoff.killAllAgents"
        },
        {
          "command": "liftoff.continueAgent"
        },
        {
          "command": "liftoff.viewHistory"
        },
        {
          "command": "liftoff.setApiKey"
        },
        {
          "command": "liftoff.buildApp"
        },
        {
          "command": "liftoff.resumeBuild"
        },
        {
          "command": "liftoff.addFeature"
        },
        {
          "command": "liftoff.deployApp"
        },
        {
          "command": "liftoff.generateTests"
        }
      ]
    },
    "configuration": {
      "title": "Liftoff",
      "properties": {
        "liftoff.defaultModel": {
          "type": "string",
          "default": "deepseek-ai/DeepSeek-V3-0324",
          "enum": [
            "deepseek-ai/DeepSeek-V3-0324",
            "Qwen/Qwen2.5-Coder-32B-Instruct",
            "deepseek-ai/DeepSeek-R1",
            "Qwen/Qwen2.5-72B-Instruct",
            "meta-llama/Llama-3.3-70B-Instruct"
          ],
          "enumDescriptions": [
            "DeepSeek V3 - Best for agents (RECOMMENDED)",
            "Qwen Coder 32B - Fast, optimized for coding",
            "DeepSeek R1 - Deep reasoning",
            "Qwen 72B - Good balance",
            "Llama 3.3 70B - Meta's flagship"
          ],
          "description": "Model for agents. DeepSeek-V3 recommended."
        },
        "liftoff.autoHandoff": {
          "type": "boolean",
          "default": true,
          "description": "Auto-spawn agents on handoff requests"
        },
        "liftoff.showAgentButtons": {
          "type": "boolean",
          "default": false,
          "description": "Show individual agent type buttons (advanced)"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "npm run test:unit",
    "test:unit": "mocha",
    "test:integration": "mocha --require ts-node/register 'tests/integration/**/*.test.ts' --timeout 30000",
    "test:e2e": "playwright test",
    "test:coverage": "c8 npm run test:unit",
    "test:watch": "mocha --require ts-node/register 'tests/unit/**/*.test.ts' --watch --watch-files src,tests"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.40.0",
    "@types/chai": "^4.3.11",
    "@types/eslint": "^9.6.1",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.0",
    "@types/sinon": "^17.0.2",
    "@types/uuid": "^10.0.0",
    "@types/vscode": "^1.85.0",
    "c8": "^9.0.0",
    "chai": "^4.3.10",
    "eslint": "^9.39.1",
    "mocha": "^10.2.0",
    "sinon": "^17.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.48.0"
  },
  "dependencies": {
    "playwright": "^1.40.0",
    "uuid": "^13.0.0"
  }
}

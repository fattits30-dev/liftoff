{
  "$schema": "https://raw.githubusercontent.com/anthropics/mcp/main/schemas/config.json",
  "servers": {
    "context7": {
      "command": "npx",
      "args": ["-y", "@context7/mcp-server"],
      "enabled": true
    },
    "serena": {
      "command": "python",
      "args": ["-m", "serena"],
      "enabled": true
    },
    "filesystem": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "."],
      "enabled": true
    },
    "github": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "enabled": true
    }
  }
}
